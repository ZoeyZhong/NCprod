@using Telerik.Web.Mvc.UI
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>



    <link rel="icon" type="image/ico" href="../../PIC/index_m/fav.ico">
    <link href="@Url.Content("~/CSS/last/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/last/bootstrap-responsive.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/last/easyui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/last/icon.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/last/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/last/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/last/jquery-1.8.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/jquery.json-2.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/last/SxManager.js")" type="text/javascript"></script>
@(Html.Telerik().StyleSheetRegistrar().DefaultGroup(group => group.DefaultPath("~/CSS/last/2012.1.214/").Add("telerik.common.css").Add("telerik.windows7.css").Combined(false).Compress(false)))
    @RenderSection("Styles", false)
    @(Html.Telerik().ScriptRegistrar().DefaultGroup(group => group.DefaultPath("~/Scripts/last/2012.1.214/").Add("telerik.common.min.js").Add("telerik.autocomplete.min.js").Combined(false).Compress(false)).jQuery(false))
</head>
<body>
    <table width="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
    <td width="17" valign="top" background="../../CSS/homeimg/mail_leftbg.gif">
        <img src="../../CSS/homeimg/left-top-right.gif" width="17" height="29" />
    </td>
    <td valign="top" background="../../CSS/homeimg/content-bg.gif">
        <table width="100%" height="31" border="0" cellpadding="0" cellspacing="0" class="left_topbg"
               id="table2">
            <tr>
                <td height="31">
                    <div class="titlebt">
                        @*◆&nbsp;&nbsp;所在位置：@(ViewBag.Title)*@
                            </div>
                        </td>
                    </tr>
                </table>
            </td>
            <td width="16" valign="top" background="../../CSS/homeimg/mail_rightbg.gif">
                <img src="../../CSS/homeimg/nav-right-bg.gif" width="16" height="29" />
            </td>
        </tr>
        <tr>
            <td valign="middle" background="../../CSS/homeimg/mail_leftbg.gif">
                &nbsp;
            </td>
            <td valign="top" bgcolor="#F7F8F9" style="min-height: 550px">
                @RenderBody()
            </td>
            <td background="../../CSS/homeimg/mail_rightbg.gif">
                &nbsp;
            </td>
        </tr>
        <tr>
            <td valign="bottom" background="../../CSS/homeimg/mail_leftbg.gif">
                <img src="../../CSS/homeimg/buttom_left2.gif" width="17" height="17" />
            </td>
            <td background="../../CSS/homeimg/buttom_bgs.gif">
                <img src="../../CSS/homeimg/buttom_bgs.gif" width="17" height="17">
            </td>
            <td valign="bottom" background="../../CSS/homeimg/mail_rightbg.gif">
                <img src="../../CSS/homeimg/buttom_right2.gif" width="16" height="17" />
            </td>
        </tr>
    </table>
@RenderSection("scripts", required: false)
</body>
</html>
