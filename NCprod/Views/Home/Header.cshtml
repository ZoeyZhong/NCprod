@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Header</title>
    <link rel="icon" type="image/ico" href="../../PIC/index_m/fav.ico">
    <link href="../../CSS/Home.css" rel="stylesheet" type="text/css" />
    <script src="../../Scripts/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            $('#loginout').click(function () {
                if (confirm("您确定要退出吗？"))
                    parent.location.href = "/Home/LogOff";
                return false;
            });
        });
    </script>
</head>
<body>
    <table width="100%" height="64" border="0" cellpadding="0" cellspacing="0" class="admin_topbg">
        <tr>
            <td width="61%" height="64">
                <a href="../../Front/Index" target="_blank"><img src="../../PIC/logo.png" width="262" height="64"></a>
            </td>
            <td width="39%" valign="top">
                <table width="100%" border="0" cellspacing="0" cellpadding="0">
                    <tr>
                        <td width="74%" height="38" class="admin_txt">
                            @((Session["user"] as NCprod.Models.dl_basic_users).exname); 您好,欢迎登陆使用！
                        </td>
                        <td width="22%" style="line-height: 59px;">
                            <a href="#" target="_self" id="loginout">
                                <img src="../../PIC/out.png" alt="安全退出" width="46" height="20" border="0" />
                            </a>
                        </td>
                        <td width="4%">
                            &nbsp;
                        </td>
                    </tr>
                    <tr>
                        <td height="19" colspan="3">
                            &nbsp;
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
</body>
</html>